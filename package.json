    "builded": "cross-env NODE_ENV=development node ./dist/server/server.js",
    "unit-test": "cross-env NODE_ENV=test mocha --opts mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts mocha.opts ./dist/tests/integration/integration.test.js",
    "prepush": "npm run lint"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.2",
    "@types/express": "^4.0.36",
