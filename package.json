    "@types/sequelize": "^4.0.68",
    "@types/supertest": "^2.0.2",
    "chai": "^4.1.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-typescript": "^3.2.1",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "sequelize": "^4.4.3",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.4",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  },
